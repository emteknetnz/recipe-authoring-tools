{
    "name": "silverstripe/recipe-authoring-tools",
    "type": "silverstripe-recipe",
    "description": "Extra tools for CMS authoring in SilverStripe",
    "homepage": "https://www.silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "4.4.0-rc1",
        "silverstripe/documentconverter": "2.0.2@stable",
        "silverstripe/iframe": "2.0.3@stable",
        "silverstripe/spellcheck": "2.1.0@stable",
        "silverstripe/tagfield": "2.3.0@stable",
        "silverstripe/taxonomy": "2.0.2@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": []
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}